# Database
POSTGRES_PASSWORD=your_secure_password_here

# Redis
REDIS_PASSWORD=your_redis_password_here

# Artemis/ActiveMQ
ARTEMIS_PASSWORD=your_artemis_password_here

# Security
JWT_SECRET=your_very_long_random_secret_key_min_256_bits

# OAuth GitHub
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_WEBHOOK_SECRET=your_webhook_secret

# BitBucket
BITBUCKET_WEBHOOK_SECRET=your_bitbucket_webhook_secret

# SonarQube (optional)
SONARQUBE_ENABLED=false
SONARQUBE_URL=https://sonarqube.example.com
SONARQUBE_TOKEN=your_sonarqube_token

# LLM Provider (openrouter or ollama)
LLM_PROVIDER=openrouter
OPENROUTER_API_KEY=sk-or-v1-your-key-here
LLM_MODEL=anthropic/claude-3.5-sonnet

# Stripe
STRIPE_API_KEY=sk_live_your_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# CORS (comma-separated list)
CORS_ORIGINS=https://pullwise.ai,https://www.pullwise.ai
